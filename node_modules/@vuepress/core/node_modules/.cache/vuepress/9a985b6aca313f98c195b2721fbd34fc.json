eSidebar){\n        this.isSidebarOpen = false\n      }\n    })\n  },\n\n  methods: {\n    toggleSidebar (to, foreCloseSidebar) {\n      this.isSidebarOpen = typeof to === 'boolean' ? to : !this.isSidebarOpen\n      this.isForeCloseSidebar = typeof foreCloseSidebar === 'boolean' ? foreCloseSidebar : false;\n      this.$emit('toggle-sidebar', this.isSidebarOpen)\n    },\n\n    toggleSidebarForce () {\n      this.isSidebarOpen = !this.isSidebarOpen\n      this.isForeCloseSidebar = !this.isForeCloseSidebar\n      this.$emit('toggle-sidebar-force', this.isSidebarOpen)\n    },\n\n    // side swipe\n    onTouchStart (e) {\n      this.touchStart = {\n        x: e.changedTouches[0].clientX,\n        y: e.changedTouches[0].clientY\n      }\n    },\n\n    onTouchEnd (e) {\n      const dx = e.changedTouches[0].clientX - this.touchStart.x\n      const dy = e.changedTouches[0].clientY - this.touchStart.y\n      if (Math.abs(dx) > Math.abs(dy) && Math.abs(dx) > 40) {\n        if (dx > 0 && this.touchStart.x <= 80) {\n          this.toggleSidebar(true)\n        } else {\n          this.toggleSidebar(false)\n        }\n      }\n    }\n  }\n}\n"],"mappings":"AACA,OAAOA,IAAI,MAAM,4BAA4B;AAC7C,OAAOC,MAAM,MAAM,8BAA8B;AACjD,OAAOC,IAAI,MAAM,4BAA4B;AAC7C,OAAOC,OAAO,MAAM,+BAA+B;AACnD,OAAOC,WAAW,MAAM,mCAAmC;AAC3D,SAASC,mBAAmB,EAAEC,cAAc,QAAQ,SAAS;AAE7D,eAAe;EACbC,UAAU,EAAE;IAAEP,IAAI;IAAEE,IAAI;IAAEC,OAAO;IAAEF,MAAM;IAAEG;EAAY,CAAC;EAExDI,IAAIA,CAAA,EAAI;IACN,OAAO;MACLC,aAAa,EAAE,KAAK;MACpBC,kBAAkB,EAAE;IACtB,CAAC;EACH,CAAC;EAEDC,QAAQ,EAAE;IACRC,gBAAgBA,CAAA,EAAI;MAClB,MAAM;QAAEC;MAAY,CAAC,GAAG,IAAI,CAACC,KAAK;MAClC,MAAM;QAAEC;MAAY,CAAC,GAAG,IAAI,CAACC,KAAK;MAClC,IACED,WAAW,CAACE,MAAM,KAAK,KAAK,IACzBJ,WAAW,CAACI,MAAM,KAAK,KAAK,EAAE;QACjC,OAAO,KAAK;MACd;MACA,OACE,IAAI,CAACC,MAAM,IACRL,WAAW,CAACM,IAAI,IAChBN,WAAW,CAACO,IAAI,IAChBP,WAAW,CAACQ,GAAG,IACf,IAAI,CAACC,kBAAkB,CAACD,GAAG;IAElC,CAAC;IAEDE,iBAAiBA,CAAA,EAAI;MACnB,MAAM;QAAER;MAAY,CAAC,GAAG,IAAI,CAACC,KAAK;MAClC,OACE,CAACD,WAAW,CAACS,IAA