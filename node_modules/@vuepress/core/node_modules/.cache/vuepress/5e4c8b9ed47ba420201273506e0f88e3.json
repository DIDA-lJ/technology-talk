\n}\n\nfunction css (el, property) {\n  // NOTE: Known bug, will return 'auto' if style value is 'auto'\n  const win = el.ownerDocument.defaultView\n  // null means not to return pseudo styles\n  return win.getComputedStyle(el, null)[property]\n}\n</script>\n\n<style lang=\"stylus\">\n$navbar-vertical-padding = 0.7rem\n$navbar-horizontal-padding = 1.5rem\n\n.navbar\n  padding $navbar-vertical-padding $navbar-horizontal-padding\n  line-height $navbarHeight - 1.4rem\n  a, span, img\n    display inline-block\n  .logo\n    height $navbarHeight - 1.4rem\n    min-width $navbarHeight - 1.4rem\n    margin-right 0.8rem\n    vertical-align top\n  .site-name\n    font-size 1.3rem\n    font-weight 600\n    color $textColor\n    position relative\n  .links\n    padding-left 1.5rem\n    box-sizing border-box\n    background-color white\n    white-space nowrap\n    font-size 0.9rem\n    position absolute\n    right $navbar-horizontal-padding\n    top $navbar-vertical-padding\n    display flex\n    .search-box\n      flex: 0 0 auto\n      vertical-align top\n\n@media (max-width: $MQMobile)\n  .navbar\n    padding-left 4rem\n    .can-hide\n      display none\n    .links\n      padding-left 1.5rem\n    .site-name\n      width calc(100vw - 9.4rem)\n      overflow hidden\n      white-space nowrap\n      text-overflow ellipsis\n</style>\n"],"mappings":"AAuCA,OAAAA,gBAAA;AACA,OAAAC,SAAA;AACA,OAAAC,aAAA;AACA,OAAAC,QAAA;AAEA;EACAC,IAAA;EAEAC,UAAA;IACAH,aAAA;IACAC,QAAA;IACAF,SAAA;IACAD;EACA;EAEAM,KAAA;IACA;MACAC,iBAAA;IACA;EACA;EAEAC,QAAA;IACAC,QAAA;MACA,YAAAC,kBAAA,CAAAD,OAAA,SAAAE,KAAA,CAAAC,WAAA,CAAAH,OAAA;IACA;IAEAI,gBAAA;MACA,YAAAJ,OAAA,SAAAA,OAAA,CAAAK,MAAA,SAAAL,OAAA,CAAAM,SAAA;IACA;EACA;EAEAC,QAAA;IACA,MAAAC,yBAAA;IACA,MAAAC,uBAAA,GAAAC,QAAA,CAAAC,GAAA,MAAAC,GAAA,oBAAAF,QAAA,CAAAC,GAAA,MAAAC,GAAA;IACA,MAAAC,oBAAA,GAAAA,CAAA;MACA,IAAAC,QAAA,CAAAC,eAAA,CAAAC,WAAA,GAAAR,yBAAA;QACA,KAAAV,iBAAA;MACA;QACA,KAAAA,iBAAA,QAAAc,GAAA,CAAAK,WAAA,GAAAR,uBAAA,IACA,KAAAS,KAAA,CAAAC,QAAA,SAAAD,KAAA,